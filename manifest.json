{
    "name": "Cookie Manager",
    "version": "2.1",
    "description": "View, search and delete cookies",
    "permissions": ["cookies", "http://*/*", "https://*/*"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "manifest_version": 2
}
